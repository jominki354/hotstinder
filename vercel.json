{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "client/build",
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/auth/bnet.js": {
      "maxDuration": 30
    },
    "api/auth/bnet/callback.js": {
      "maxDuration": 30
    },
    "api/auth/me.js": {
      "maxDuration": 30
    },
    "api/auth/admin-login.js": {
      "maxDuration": 30
    },
    "api/admin.js": {
      "maxDuration": 60
    },
    "api/admin/dashboard.js": {
      "maxDuration": 30
    },
    "api/debug/env.js": {
      "maxDuration": 30
    },
    "api/test.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/admin/users",
      "destination": "/api/admin"
    },
    {
      "source": "/api/admin/matches",
      "destination": "/api/admin"
    },
    {
      "source": "/api/admin/delete-all-users",
      "destination": "/api/admin"
    },
    {
      "source": "/api/admin/delete-all-matches",
      "destination": "/api/admin"
    },
    {
      "source": "/api/admin/create-test-accounts",
      "destination": "/api/admin"
    },
    {
      "source": "/api/admin/create-test-matches",
      "destination": "/api/admin"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}
