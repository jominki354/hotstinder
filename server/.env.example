# 서버 설정
PORT=5000
NODE_ENV=development

# MongoDB 연결 설정
MONGODB_URI=mongodb://localhost:27017/hots-matchmaking

# JWT 시크릿 키 (랜덤 문자열로 변경 필요)
JWT_SECRET=your_jwt_secret_key_here
JWT_EXPIRES_IN=7d

# 배틀넷 OAuth 설정
BNET_CLIENT_ID=your_battle_net_client_id
BNET_CLIENT_SECRET=your_battle_net_client_secret
BNET_CALLBACK_URL=http://localhost:3000/auth/callback
BNET_REGION=kr

# 프론트엔드 URL
FRONTEND_URL=http://localhost:3000

# 세션 시크릿 (랜덤 문자열로 변경 필요)
SESSION_SECRET=your_session_secret_key_here 